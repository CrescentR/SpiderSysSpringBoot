<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multiSpider.mapper.SpiderTaskMapper">

    <resultMap id="BaseResultMap" type="com.multiSpider.entity.SpiderTask">
        <id property="id" column="id" />
        <result property="name" column="name" />
        <result property="description" column="description" />
        <result property="keywords" column="keywords"
                typeHandler="com.baomidou.mybatisplus.extension.handlers.JacksonTypeHandler"/>
        <result property="maxPages" column="max_pages" />
        <result property="timeout" column="timeout" />
        <result property="status" column="status" />
        <result property="createdAt" column="created_at" />
        <result property="updatedAt" column="updated_at" />
    </resultMap>


    <sql id="Base_Column_List">
        id,name,description,keywords,seed_urls,max_pages,
        timeout,status,created_at,updated_at
    </sql>
    <select id="selectTaskName" resultType="com.multiSpider.entity.TaskName">
        SELECT id, name FROM spider_task
    </select>
</mapper>
